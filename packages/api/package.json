{
  "name": "@frilan/api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node build/index.js",
    "build": "tsc",
    "dev": "tsnd --no-notify --transpile-only src/index.ts",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "mig:gen": "npm run typeorm migration:generate -- -p -n",
    "mig:run": "npm run typeorm migration:run"
  },
  "dependencies": {
    "pg": "^8.5.1",
    "typeorm": "^0.2.30"
  },
  "devDependencies": {
    "@types/node": "^14.14.22",
    "ts-node-dev": "^1.1.1"
  }
}
