{
  "env": {
    "browser": true
  },
  "extends": ["plugin:vue/vue3-recommended"],
  "plugins": ["vue"],
  "overrides": [
    {
      "files": ["*.vue"],
      "rules": {
        "indent": ["error", 2],
        "@typescript-eslint/no-unused-vars": "off"
      }
    }
  ],
  "globals": {
    "defineProps": "readonly",
    "defineEmits": "readonly",
    "defineExpose": "readonly",
    "withDefaults": "readonly",
    "$": "readonly",
    "$$": "readonly",
    "$ref": "readonly",
    "$computed": "readonly",
    "$shallowRef": "readonly"
  }
}
