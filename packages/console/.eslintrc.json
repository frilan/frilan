{
  "env": {
    "browser": true
  },
  "extends": ["plugin:vue/vue3-recommended"],
  "plugins": ["vue"],
  "overrides": [
    {
      "files": ["*.vue"],
      "rules": {"indent": ["error", 2]}
    }
  ],
  "globals": {
    "defineProps": "readonly",
    "defineEmits": "readonly",
    "defineExpose": "readonly",
    "withDefaults": "readonly",
    "$ref": "readonly",
    "$computed": "readonly",
    "$shallowRef": "readonly"
  }
}
